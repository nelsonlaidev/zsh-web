{
  "name": "zsh-web",
  "version": "0.0.0",
  "license": "MIT",
  "author": "Nelson Lai <me@nelsonlai.dev> (https://github.com/nelsonlaidev/)",
  "type": "module",
  "scripts": {
    "build": "next build --turbo",
    "check": "pnpm lint && pnpm typecheck && pnpm format:check && pnpm knip",
    "clean": "rm -rf .next",
    "dev": "next dev --turbo",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "preinstall": "npx --yes only-allow pnpm",
    "knip": "knip",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint --fix . --max-warnings 0",
    "prepare": "lefthook install",
    "start": "next start",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@t3-oss/env-nextjs": "^0.13.8",
    "@zenfs/core": "^2.4.0",
    "@zenfs/dom": "^1.2.4",
    "arg": "^5.0.2",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.18",
    "kerium": "^1.3.6",
    "next": "15.5.9",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "tailwind-merge": "^3.3.1",
    "uid": "^2.0.2",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@nelsonlaidev/eslint-config": "^1.0.0",
    "@nelsonlaidev/prettier-config": "^0.1.0",
    "@nelsonlaidev/typescript-config": "^0.0.3",
    "@tailwindcss/postcss": "^4.1.13",
    "@types/node": "^24.3.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "eslint": "^9.35.0",
    "knip": "^5.73.4",
    "lefthook": "^2.0.2",
    "postcss-load-config": "^6.0.1",
    "prettier": "^3.6.2",
    "prettier-plugin-packagejson": "^2.5.19",
    "tailwindcss": "^4.1.13",
    "typescript": "^5.9.2"
  },
  "packageManager": "pnpm@10.15.1",
  "engines": {
    "node": ">=22"
  }
}
